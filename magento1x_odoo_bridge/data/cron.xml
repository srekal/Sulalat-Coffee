<odoo>

    <record id="cron_import_categories_from_magento1x" forcecreate='True' model="ir.cron">
        <field name="name">Magento1x - Import Categories</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1500</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="model_id" ref="model_import_magento1x_categories"/>
        <field name="state">code</field>
        <field name="code">model._cron_magento1x_import_categories()</field>
    </record>

